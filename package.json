{
	"name": "app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"gen-config": "dotenv -- envsubst < static/config.template.js > static/config.js",
		"_gen-openapi-ts": "pnpm openapi-typescript $PUBLIC_API_URL/openapi.json --output ./src/lib/types/api.ts",
		"gen-openapi-ts": "dotenv -- pnpm _gen-openapi-ts",
		"dev": "pnpm gen-config && vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@lucide/svelte": "^0.537.0",
		"@sevria/ui": "^0.0.5",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/js-cookie": "^3.0.6",
		"dotenv-cli": "^10.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"felte": "^1.3.0",
		"globals": "^16.0.0",
		"js-cookie": "^3.0.5",
		"openapi-fetch": "^0.14.0",
		"openapi-typescript": "^7.8.0",
		"openapi-typescript-fetch": "^2.2.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"wait": "^0.4.2"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
